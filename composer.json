{
    "name": "briantweed/laravelviewfolder",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Tweed",
            "email": "brtweed@outlook.com",
            "homepage": "n/a"
        }
    ],
    "homepage": "https://github.com/briantweed/laravelviewfolder",
    "keywords": ["Laravel", "LaravelViewFolder"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "briantweed\\LaravelViewFolder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "briantweed\\LaravelViewFolder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "briantweed\\LaravelViewFolder\\LaravelViewFolderServiceProvider"
            ],
            "aliases": {
                "LaravelViewFolder": "briantweed\\LaravelViewFolder\\Facades\\LaravelViewFolder"
            }
        }
    }
}
